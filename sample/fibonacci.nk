fn main() -> void
{
	let:i32 x = 10;
	let:i32 i = 0;

	for(i -> x :: 1 )
	{
		let:i32 c = fibonacci(i);
		println(c);
	}
	return;
}

fn fibonacci(i32:n) -> i32
{
	if( n == 0 )
	{
		return n;
	}
	if( n == 1 )
	{
		return n;
	}
	let:i32 arg_1 = n;
	arg_1 -= 1;
	let:i32 arg_2 = n;
	arg_2 -= 2;
	let:i32 a = fibonacci( arg_1 );
	let:i32 b = fibonacci( arg_2 );
	a += b;
	return a;
}
